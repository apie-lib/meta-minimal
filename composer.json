{
  "name": "apie/meta-minimal",
  "description": "Composer package of the apie library: meta minimal",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\MetaMinimal\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\MetaMinimal\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "apie/apie-common-plugin": "self.version",
    "apie/core": "self.version",
    "apie/rest-api": "self.version"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6.19"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}